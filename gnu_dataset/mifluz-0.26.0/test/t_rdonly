. ./test_functions

export MIFLUZ_CONFIG ; MIFLUZ_CONFIG=${srcdir}/mifluz-dict.conf
../htdb/mifluzload -z $VERBOSE test < $srcdir/dict.txt
chmod a-w test
a=`./dbbench -Wz -f world`
b="world	0	0	5	2	
world	0	0	5	4	
world	0	0	11	1	
world	0	0	20	5	
world	0	0	21	2	
world	0	0	51	2	
world	0	0	71	2	
world	0	0	81	2	"
if [ "$a" != "$b" ]
then
	echo "expected
$b
but got
$a" >&2
	exit 1
fi
