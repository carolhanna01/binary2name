. ./test_functions

if [ "$LONG" ]
then
	rm -f test
	time ./dbbench -w ${srcdir}/cache.txt -B test -k 3000000
else
	echo "SKIP: t_largefile (run with make LONG=yes TESTS=t_largefile check)"
	exit 77
fi
