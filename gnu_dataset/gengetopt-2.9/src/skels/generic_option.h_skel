case '@short_option@':	/* @option_comment@.  */
  if (args_info->@option_var_name@_given)
    {
      fprintf (stderr, "%s: `--@long_option@' (`-@short_option@') option given more than once\n", PACKAGE);
      clear_args ();
      @gen_exit@ (EXIT_FAILURE);
    }
  args_info->@option_var_name@_given = 1;
  