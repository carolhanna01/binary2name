<?xml version="1.0" encoding="UTF-8"?>
<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row type="scheme">
        <action>ToggleNonPrinting</action>
        <after>WholeMeasureSpacer</after>
        <_label>Spacer/Normal Rest</_label>
        <_tooltip>Makes the rest at the cursor a spacer rest/restores it as a printing rest.</_tooltip>
      </row>
    </map>
  </merge>
</Denemo>
