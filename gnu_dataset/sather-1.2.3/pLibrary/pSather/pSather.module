    -- psather.sa -has psather.sa $LOCK GATE MUTEX
    lm_access.sa -has lm_access.sa	LM_LOCK_MANAGER_ACCESS
					LM_LOCK_C_ARRAY
    lm_combined.sa -has lm_combined.sa	LM_REF_C_INT LM_COMBINED_LOCKS_MANAGER
    lm_external.sa
    lm_include.sa -has lm_include.sa	LM_SINGLE_LOCK_MANAGER_INCLUDE
					LM_SINGLE_MUTEX_MANAGER_INCLUDE
    lm_queue.sa -has lm_queue.sa	$LM_THREAD_HANDLE_EXTENSION
					LM_THREAD_HANDLE_EXTENSION
					LM_THREAD_HANDLE LM_THREAD_HANDLE_QUEUE
    lm_single.sa -has lm_single.sa	LM_SINGLE_LOCK_MANAGER
					LM_MULTIPLE_LOCK_MANAGER
    attach.sa -has attach.sa		PAR_ATTACH ATTACH_INCLUDE ATTACH
    gates.sa -has gates.sa		GATE 
    mutex.sa -has mutex.sa		MUTEX $MUTEX MUTEX_INCLUDE
					SMUTEX CMUTEX QMUTEX
    pabstract.sa 
    pid.sa -has pid.sa			THREAD_ID
    timing.sa -has timing.sa		TIMING
    future.sa -has future.sa		FUTURE
    rw_lock.sa -has rw_lock.sa		$READER_LOCK $WRITER_LOCK $RW_LOCK
					RW_LOCK FRW_LOCK WR_LOCK FWR_LOCK
    barrier.sa -has barrier.sa		DOOR_LOCK $BARRIER_LOCK
    rendezvous.sa -has rendezvous.sa	RENDEZVOUS_LOCK
    vm.sa -has vm.sa			VISITOR_MUTATOR
    ll_lock.sa -has ll_lock.sa		LL_LOCK
    cond_locks.sa -has cond_locks.sa	$COND_LOCK COND_LOCK COND_LOCK_HELPER
    spinlock.sa -has spinlock.sa	SPINLOCK
    sema.sa -has sema.sa		SEMA

    gates_test.sa -has gates_test.sa		TEST_GATES
    locks_test.sa -has locks_test.sa		TEST_LOCKS
    copy_test.sa -has copy_test.sa		TEST_COPY
    future_test.sa -has future_test.sa		TEST_FUTURE
    rw_test.sa -has rw_test.sa			TEST_RW
    barrier_test.sa -has barrier_test.sa	TEST_DOOR
    rv_test.sa -has rv_test.sa			TEST_RV
    shared_test.sa -has shared_test.sa		TEST_SHARED
    par_test.sa -has par_test.sa		TEST_PAR
    remote_test.sa -has remote_test.sa		TEST_REMOTE
    cluster_test.sa -has cluster_test.sa	TEST_CLUSTER
    yield_test.sa -has yield_test.sa		TEST_YIELD
    ll_lock_test.sa -has ll_lock_test.sa	TEST_LL_LOCK
    spinlock_test.sa -has spinlock_test.sa	TEST_SPINLOCK
    mutex_test.sa -has mutex_test.sa		TEST_MUTEX
    sema_test.sa -has sema_test.sa		TEST_SEMA

