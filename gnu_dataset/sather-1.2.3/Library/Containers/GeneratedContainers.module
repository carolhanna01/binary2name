-- Classes that are generated from the pSather version.  These classes
-- are not included into the general module so that pSather does not
-- encounter these classes twice.

    stack.sa -has stack.sa $STACK 
    a_stack.sa -has a_stack.sa A_STACK STACK 
    nr_stack.sa -has nr_stack.sa $NR_STACK
    nr_a_stack.sa -has nr_a_stack.sa NR_A_STACK NR_STACK

