-- Author: Benedict A. Gomes <gomes@samosa.ICSI.Berkeley.EDU>

class TUP_TEST is
   include TEST;
   
   main is
      class_name("TUP");
      f: TUP{INT,INT} := #(1,3);
      test("str",f,"{1,3}");
      g: TUP{INT,STR} := #(1,"test");
      test("str",g,"{1,test}");
      h: TUP{INT} := #(1);
      test("str",h,"{1}");
      i: TUP{INT,STR,INT} := #(1,"test",5);
      test("str",i,"{1,test,5}");
      j: TUP{INT,STR,INT,STR} := #(1,"test",5,"test3");
      test("str",j,"{1,test,5,test3}");
      finish;
   end;
   
end; 
