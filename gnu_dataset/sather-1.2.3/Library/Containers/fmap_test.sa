-- Author: Benedict A. Gomes <gomes@samosa.ICSI.Berkeley.EDU>

class FMAP_TEST is
   include TEST;
   
   main is
      class_name("FMAP");
      f ::= #FMAP{INT,STR};
      f := f.insert(1,"test1");
      f := f.insert(2,"test2");
      test("string version",f.str,"{[1]= test1  [2]= test2  }");
      finish;
   end;
	 
end; 
