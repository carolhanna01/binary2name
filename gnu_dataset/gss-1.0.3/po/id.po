#, c-format
msgid ""
"   MSB                                                                 LSB\n"
"   +-----------------+-----------------+---------------------------------+\n"
"   |  Calling Error  |  Routine Error  |       Supplementary Info        |\n"
"   | "
msgstr ""
"   MSB                                                                 LSB\n"
"   +-------------------+---------------+---------------------------------+\n"
"   |  Galat Memanggil  |  Galat Rutin  |       Info Tambahan             |\n"
"   | "

#: src/gss.c:138
#, c-format
msgid ""
"|\n"
"   +-----------------+-----------------+---------------------------------+\n"
"Bit 31            24  23            16  15                             0\n"
"\n"
msgstr ""
"|\n"
"   +-----------------+-----------------+---------------------------------+\n"
"Bita 31            24  23            16  15                             0\n"
"\n"

