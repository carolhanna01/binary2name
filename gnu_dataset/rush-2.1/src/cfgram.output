Grammar

    0 $accept: rcfile $end

    1 rcfile: skipeol select

    2 select: preface skipeol content
    3       | "erroneous token"

    4 preface: "rush" T_VERSION "end of line"

    5 skipeol: /* empty */
    6        | eol

    7 eol: "end of line"
    8    | eol "end of line"

    9 content: /* empty */
   10        | rulelist

   11 rulelist: rule
   12         | rulelist rule

   13 rule: rulehdr rulebody
   14     | globhdr globbody

   15 globhdr: "global" eol

   16 globbody: glob_stmt
   17         | globbody glob_stmt

   18 glob_stmt: "global attribute" arglist eol

   19 arglist: arg
   20        | arglist arg

   21 arg: literal
   22    | "number"

   23 rulehdr: "rule" ruleid eol

   24 ruleid: /* empty */
   25       | string

   26 rulebody: stmt
   27         | rulebody stmt

   28 stmt: match_stmt eol
   29     | set_stmt eol
   30     | map_stmt eol
   31     | delete_stmt eol
   32     | limits_stmt eol
   33     | environ_stmt eol
   34     | flowctl_stmt eol
   35     | attrib_stmt eol
   36     | remopt_stmt eol
   37     | include_stmt skipeol
   38     | error

   39 match_stmt: "match" compound_cond

   40 compound_cond: simple_cond
   41              | compound_cond "&&" simple_cond
   42              | compound_cond "||" simple_cond

   43 simple_cond: expr
   44            | "!" simple_cond
   45            | '(' compound_cond ')'

   46 expr: string '~' regex
   47     | string "!~" regex
   48     | string "==" literal
   49     | string "!=" literal
   50     | string "==" "number"
   51     | string "!=" "number"
   52     | string "<" "number"
   53     | string "<=" "number"
   54     | string ">" "number"
   55     | string ">=" "number"
   56     | string "in" strlist
   57     | "group" string
   58     | "group" strlist

   59 literal: "identifier"
   60        | "string"

   61 string: "identifier"
   62       | "string"
   63       | "number"

   64 regex: string

   65 set_stmt: "set" index '=' value
   66         | "insert" index '=' value
   67         | "set" index "=~" value
   68         | "set" index '=' string '~' value
   69         | "insert" index '=' string '~' value
   70         | "set" "identifier" '=' value
   71         | "set" "identifier" "=~" value
   72         | "set" "identifier" '=' string '~' value
   73         | "unset" "identifier"
   74         | "unset" index

   75 map_stmt: "map" index value value value "number" "number" defval
   76         | "map" "identifier" value value value "number" "number" defval

   77 defval: /* empty */
   78       | string

   79 index: '[' "number" ']'

   80 value: string

   81 flowctl_stmt: "fallthrough"
   82             | "exit" fdescr "string"
   83             | "exit" fdescr "identifier"

   84 fdescr: /* empty */
   85       | "number"

   86 delete_stmt: "delete" range

   87 range: "number"
   88      | "number" "number"

   89 include_stmt: "include" string "end of line"

   90 limits_stmt: "limits" resource_limits

   91 resource_limits: "identifier"
   92                | resource_limits "identifier"

   93 remopt_stmt: "remopt" string optstring

   94 optstring: /* empty */
   95          | string

   96 environ_stmt: "clrenv"
   97             | "setenv" "identifier" '=' string
   98             | "evalenv" string
   99             | "unsetenv" asgn_list
  100             | "keepenv" asgn_list

  101 asgn_list: asgn
  102          | asgn_list asgn

  103 asgn: literal
  104     | "identifier" '=' value

  105 attrib_stmt: "rule attribute" string

  106 $@1: /* empty */

  107 strlist: '(' $@1 arglist ')'


