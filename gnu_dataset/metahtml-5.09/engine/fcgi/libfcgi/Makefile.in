SHELL = /bin/sh
CC	= @CC@
INCLUDES  = -I../include -I../../../
CFLAGS	= -DHAVE_CONFIG_H $(INCLUDES)
RANLIB	= @RANLIB@

OBJS	    = fcgiapp.o fcgi_stdio.o os_unix.o $(LIBOBJS)

.c.o:
	$(CC) $(CFLAGS) -DHAVE_CONFIG_H $(INCLUDES) -c $<

all: libfcgi.a

libfcgi.a: $(OBJS)
	ar cr libfcgi.a $(OBJS)
	$(RANLIB) libfcgi.a

clean:
	rm -f *.a *.o core.* errs *~ \#* TAGS *.E a.out

# ----------------------------------------------------------------------------

fcgiapp.o: fcgiapp.c

fcgi_stdio.o: fcgi_stdio.c

os_unix.o: os_unix.c

