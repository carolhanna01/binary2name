directory.h:
	echo "Making directory.h"
	@rm -f directory.h;echo '#define __LIBDIR__ "${exec_prefix}/lib"' > directory.h ; echo '#define __PERL_LOCATION__ "/usr/bin/perl"' >> directory.h; echo '#define __BINDIR__ "${exec_prefix}/bin"' >> directory.h ; echo '#define __DATADIR__ "${prefix}/share"'    >> directory.h; echo '#define __LIBDIR__  "${exec_prefix}/lib"'     >> directory.h

all: directory.h

dist-hook: 
	rm $(distdir)/directory.h

install-data-hook:
	$(INSTALL_DATA) $(srcdir)/gift-config.h $(includedir)/libMRML/include/gift-config.h
