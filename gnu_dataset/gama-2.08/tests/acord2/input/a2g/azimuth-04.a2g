^ B              # azimuth to   B
^ A              # azimuth to   A 1st
^ A              # azimuth to   A 2nd
^ A              # azimuth to   A 3d
> A              # direction to A
> B              # direction to B
> D              # direction to D
)

( D              # standpoint  D
