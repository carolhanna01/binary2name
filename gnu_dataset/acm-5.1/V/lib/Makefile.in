# Programs to make tags files.
  ETAGS = etags -tw
  CTAGS = ctags -tw
tagsrcs = $(SRCS)

all:	$(PROGRAMS)

libV.a:	$(OBJS)
	rm -f $@
	$(AR) $@ $(OBJS)
	$(RANLIB) $@

clean:
	rm -f $(PROGRAMS) *.o core *~ TAGS tags *.bak

mostlyclean:
	rm -f *.o core *~

TAGS:	$(tagsrcs)
	$(ETAGS) $(tagsrcs)

tags:   $(tagsrcs)
	$(CTAGS) $(tagsrcs)

distclean: clean
	rm -f Makefile

realclean:	distclean
	rm -f TAGS

install uninstall:

depend:
	$(MAKEDEPEND) $(DEFINES) @DEFS@ $(STD_INCLUDES) $(SRCS)

test check:

