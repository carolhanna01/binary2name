#, scheme-format
msgid "  Requires ~a.~%"
msgstr "  Requiere ~a.~%"

#: modules/shepherd/scripts/herd.scm:113
#, scheme-format
msgid "  Conflicts with ~a.~%"
msgstr "  Tiene un conflicto con ~a.~%"

