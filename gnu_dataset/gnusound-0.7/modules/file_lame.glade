<?xml version="1.0"?>
<GTK-Interface>

<project>
  <name>file_lame</name>
  <program_name>file_lame</program_name>
  <directory></directory>
  <source_directory>src</source_directory>
  <pixmaps_directory>pixmaps</pixmaps_directory>
  <language>C</language>
  <gnome_support>True</gnome_support>
  <gettext_support>True</gettext_support>
</project>

<widget>
  <class>GtkWindow</class>
  <name>lame_config</name>
  <visible>False</visible>
  <title>window1</title>
  <type>GTK_WINDOW_TOPLEVEL</type>
  <position>GTK_WIN_POS_NONE</position>
  <modal>False</modal>
  <allow_shrink>False</allow_shrink>
  <allow_grow>True</allow_grow>
  <auto_shrink>False</auto_shrink>

  <widget>
    <class>GtkTable</class>
    <name>lame_config_panel</name>
    <border_width>8</border_width>
    <rows>4</rows>
    <columns>1</columns>
    <homogeneous>False</homogeneous>
    <row_spacing>8</row_spacing>
    <column_spacing>0</column_spacing>

    <widget>
      <class>GtkFrame</class>
      <name>lame_bitrate_settings</name>
      <label>Bitrate</label>
      <label_xalign>0</label_xalign>
      <shadow_type>GTK_SHADOW_ETCHED_IN</shadow_type>
      <child>
	<left_attach>0</left_attach>
	<right_attach>1</right_attach>
	<top_attach>1</top_attach>
	<bottom_attach>2</bottom_attach>
	<xpad>0</xpad>
	<ypad>0</ypad>
	<xexpand>False</xexpand>
	<yexpand>False</yexpand>
	<xshrink>False</xshrink>
	<yshrink>False</yshrink>
	<xfill>True</xfill>
	<yfill>True</yfill>
      </child>

      <widget>
	<class>GtkVBox</class>
	<name>vbox1</name>
	<border_width>8</border_width>
	<homogeneous>False</homogeneous>
	<spacing>0</spacing>

	<widget>
	  <class>GtkHScale</class>
	  <name>lame_bitrate</name>
	  <can_focus>True</can_focus>
	  <draw_value>True</draw_value>
	  <value_pos>GTK_POS_TOP</value_pos>
	  <digits>0</digits>
	  <policy>GTK_UPDATE_CONTINUOUS</policy>
	  <value>192</value>
	  <lower>32</lower>
	  <upper>320</upper>
	  <step>8</step>
	  <page>8</page>
	  <page_size>0</page_size>
	  <child>
	    <padding>0</padding>
	    <expand>True</expand>
	    <fill>True</fill>
	  </child>
	</widget>

	<widget>
	  <class>GtkCheckButton</class>
	  <name>lame_bitrate_cbr</name>
	  <tooltip>If enabled, all passages will use the same bitrate. This is most useful for streaming applications. If disabled, the encoder will adapt the bitrate to the complexity of the sound, leading to better quality</tooltip>
	  <can_focus>True</can_focus>
	  <label>Use constant bitrate</label>
	  <active>False</active>
	  <draw_indicator>True</draw_indicator>
	  <child>
	    <padding>0</padding>
	    <expand>False</expand>
	    <fill>False</fill>
	  </child>
	</widget>
      </widget>
    </widget>

    <widget>
      <class>GtkFrame</class>
      <name>lame_quality_settings</name>
      <label>Quality</label>
      <label_xalign>0</label_xalign>
      <shadow_type>GTK_SHADOW_ETCHED_IN</shadow_type>
      <child>
	<left_attach>0</left_attach>
	<right_attach>1</right_attach>
	<top_attach>2</top_attach>
	<bottom_attach>3</bottom_attach>
	<xpad>0</xpad>
	<ypad>0</ypad>
	<xexpand>False</xexpand>
	<yexpand>False</yexpand>
	<xshrink>False</xshrink>
	<yshrink>False</yshrink>
	<xfill>True</xfill>
	<yfill>True</yfill>
      </child>

      <widget>
	<class>GtkVBox</class>
	<name>vbox2</name>
	<border_width>8</border_width>
	<homogeneous>False</homogeneous>
	<spacing>0</spacing>

	<widget>
	  <class>GtkHScale</class>
	  <name>lame_quality</name>
	  <can_focus>True</can_focus>
	  <draw_value>True</draw_value>
	  <value_pos>GTK_POS_TOP</value_pos>
	  <digits>0</digits>
	  <policy>GTK_UPDATE_CONTINUOUS</policy>
	  <value>9</value>
	  <lower>0</lower>
	  <upper>9</upper>
	  <step>0</step>
	  <page>0</page>
	  <page_size>0</page_size>
	  <child>
	    <padding>0</padding>
	    <expand>True</expand>
	    <fill>True</fill>
	  </child>
	</widget>

	<widget>
	  <class>GtkHBox</class>
	  <name>hbox1</name>
	  <homogeneous>False</homogeneous>
	  <spacing>0</spacing>
	  <child>
	    <padding>0</padding>
	    <expand>False</expand>
	    <fill>True</fill>
	  </child>

	  <widget>
	    <class>GtkLabel</class>
	    <name>label1</name>
	    <label>Variable bitrate mode: </label>
	    <justify>GTK_JUSTIFY_CENTER</justify>
	    <wrap>False</wrap>
	    <xalign>0.5</xalign>
	    <yalign>0.5</yalign>
	    <xpad>0</xpad>
	    <ypad>0</ypad>
	    <child>
	      <padding>0</padding>
	      <expand>False</expand>
	      <fill>False</fill>
	    </child>
	  </widget>

	  <widget>
	    <class>GtkRadioButton</class>
	    <name>lame_vbr_mode_new</name>
	    <tooltip>Use the &quot;new&quot; VBR algorithm. This algorithm is very fast but less well-tested than the &quot;old&quot; algorithm.</tooltip>
	    <can_focus>True</can_focus>
	    <label>New</label>
	    <active>True</active>
	    <draw_indicator>True</draw_indicator>
	    <group>lame_vbr_mode</group>
	    <child>
	      <padding>0</padding>
	      <expand>False</expand>
	      <fill>False</fill>
	    </child>
	  </widget>

	  <widget>
	    <class>GtkRadioButton</class>
	    <name>lame_vbr_mode_old</name>
	    <tooltip>Use the &quot;old&quot; VBR algorithm. This algorithm is slower than the &quot;new&quot; algorithm but very well tested.</tooltip>
	    <can_focus>True</can_focus>
	    <label>Old</label>
	    <active>False</active>
	    <draw_indicator>True</draw_indicator>
	    <group>lame_vbr_mode</group>
	    <child>
	      <padding>0</padding>
	      <expand>False</expand>
	      <fill>False</fill>
	    </child>
	  </widget>
	</widget>
      </widget>
    </widget>

    <widget>
      <class>GtkHBox</class>
      <name>hbox2</name>
      <homogeneous>False</homogeneous>
      <spacing>8</spacing>
      <child>
	<left_attach>0</left_attach>
	<right_attach>1</right_attach>
	<top_attach>0</top_attach>
	<bottom_attach>1</bottom_attach>
	<xpad>0</xpad>
	<ypad>0</ypad>
	<xexpand>False</xexpand>
	<yexpand>False</yexpand>
	<xshrink>False</xshrink>
	<yshrink>False</yshrink>
	<xfill>False</xfill>
	<yfill>False</yfill>
      </child>

      <widget>
	<class>GtkFrame</class>
	<name>frame4</name>
	<label>Target</label>
	<label_xalign>0</label_xalign>
	<shadow_type>GTK_SHADOW_ETCHED_IN</shadow_type>
	<child>
	  <padding>0</padding>
	  <expand>True</expand>
	  <fill>True</fill>
	</child>

	<widget>
	  <class>GtkVBox</class>
	  <name>vbox3</name>
	  <border_width>8</border_width>
	  <homogeneous>False</homogeneous>
	  <spacing>0</spacing>

	  <widget>
	    <class>GtkRadioButton</class>
	    <name>lame_target_bitrate</name>
	    <tooltip>Use average bitrate (ABR) or constant bitrate (CBR) encoding</tooltip>
	    <can_focus>True</can_focus>
	    <label>Bitrate (ABR/CBR)</label>
	    <active>False</active>
	    <draw_indicator>True</draw_indicator>
	    <group>lame_target</group>
	    <child>
	      <padding>0</padding>
	      <expand>False</expand>
	      <fill>False</fill>
	    </child>
	  </widget>

	  <widget>
	    <class>GtkRadioButton</class>
	    <name>lame_target_quality</name>
	    <tooltip>Use variable bitrate (VBR) encoding</tooltip>
	    <can_focus>True</can_focus>
	    <label>Quality (VBR)</label>
	    <active>False</active>
	    <draw_indicator>True</draw_indicator>
	    <group>lame_target</group>
	    <child>
	      <padding>0</padding>
	      <expand>False</expand>
	      <fill>False</fill>
	    </child>
	  </widget>
	</widget>
      </widget>

      <widget>
	<class>GtkFrame</class>
	<name>frame5</name>
	<label>Encoder</label>
	<label_xalign>0</label_xalign>
	<shadow_type>GTK_SHADOW_ETCHED_IN</shadow_type>
	<child>
	  <padding>0</padding>
	  <expand>False</expand>
	  <fill>False</fill>
	</child>

	<widget>
	  <class>GtkVBox</class>
	  <name>lame_encoder_options</name>
	  <border_width>8</border_width>
	  <homogeneous>False</homogeneous>
	  <spacing>0</spacing>

	  <widget>
	    <class>GtkCheckButton</class>
	    <name>lame_encoder_mono</name>
	    <tooltip>If set, the output will be monaural</tooltip>
	    <can_focus>True</can_focus>
	    <label>Mono encoding</label>
	    <active>False</active>
	    <draw_indicator>True</draw_indicator>
	    <child>
	      <padding>0</padding>
	      <expand>False</expand>
	      <fill>False</fill>
	      <pack>GTK_PACK_END</pack>
	    </child>
	  </widget>
	</widget>
      </widget>
    </widget>

    <widget>
      <class>GtkLabel</class>
      <name>label2</name>
      <label>Using L.A.M.E. encoder from http://www.mp3dev.org</label>
      <justify>GTK_JUSTIFY_CENTER</justify>
      <wrap>False</wrap>
      <xalign>0</xalign>
      <yalign>0.5</yalign>
      <xpad>4</xpad>
      <ypad>8</ypad>
      <child>
	<left_attach>0</left_attach>
	<right_attach>1</right_attach>
	<top_attach>3</top_attach>
	<bottom_attach>4</bottom_attach>
	<xpad>0</xpad>
	<ypad>0</ypad>
	<xexpand>False</xexpand>
	<yexpand>False</yexpand>
	<xshrink>False</xshrink>
	<yshrink>False</yshrink>
	<xfill>True</xfill>
	<yfill>False</yfill>
      </child>
    </widget>
  </widget>
</widget>

</GTK-Interface>
