header {
        lesson_id = "caa223fc-3daf-4ff2-ae4f-f4825a482202"
        module = chordvoicing
        title = _("Altered chords (chord voicing)")
}

##################################################
question {
 name = "major 7+ 5-"
 chord("c' e' ges' b'")
}

##################################################
question {
 name = "minor 7+"
 chord("c' ees' g' b'")
}

##################################################
question {
 name = "7"
 chord("c' e' g' bes'")
}

##################################################
question {
 name = "7 5-"
 chord("c' e' ges' bes'")
}

##################################################
question {
 name = "7 5+"
 chord("c' e' gis' bes'")
}

##################################################
question {
 name = "9"
 chord("c' e' g' bes' d''")
}

##################################################
question {
 name = "9+"
 chord("c' e' g' bes' dis''")
}

##################################################
question {
 name = "9-"
 chord("c' e' g' bes' des''")
}

##################################################
question {
 name = "5+ 9-"
 chord("c' e' gis' bes' des''")
}
