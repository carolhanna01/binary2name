#! /bin/sh
set -e

case "$1" in
    remove|upgrade|deconfigure)
    ;;

    failed-upgrade)
    if [ "$2" = "3.0.6-1" ]; then
        # 3.0.6-1 had a broken useage of update-python-modules
        exit 0
    fi
    ;;

    *)
        echo "prerm called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac

#DEBHELPER#

exit 0
